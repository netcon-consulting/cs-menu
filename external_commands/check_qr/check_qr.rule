name_rule
Detect QR-code

dependencies
zbar

path_cmd
/opt/cs-gateway/scripts/netcon/check_qr.sh

timeout
60

media_types
TIFF
PAM
ICO
INDD
PCX
FLI
SVM
FLC
PSD
WPG
PICT
MDI
JBIG2
PPM
JPEG
HEIF
BMP
GRIB
RAW
GIF
WMF
PGM
PSP
CUR
NITF
PNG
PBM

return_codes
0,none,No QR-code with URL detected
1,detected,QR-code with URL detected (%LOGTEXT%)
99,not_checked,Error (%LOGTEXT%)

disposal_actions
modified,none,none
detected,QR-code,none
